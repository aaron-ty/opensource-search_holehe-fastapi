
# ===== .env.example =====
# Application
APP_NAME=opensource_search_holehe_fastapi
APP_VERSION=2.0.0
MODE=development
DEBUG=true

# Server
HOST=0.0.0.0
PORT=8000
WORKERS=1

# API
API_V1_PREFIX=/api/v2

# HTTP Client
HTTP_TIMEOUT=50
HTTP_VERIFY_SSL=false
MAX_CONCURRENT_REQUESTS=50

# Validator Service (Optional)
VALIDATOR_URL=
VALIDATOR_ENABLED=false

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - [%(levelname)s] - %(message)s

# CORS
CORS_ENABLED=false
CORS_ORIGINS=["*"]